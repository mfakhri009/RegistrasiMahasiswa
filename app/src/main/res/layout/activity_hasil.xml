w<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HasilActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Data Mahasiswa"
        android:textSize="20dp"
        android:layout_marginTop="220dp"
        android:textColor="#000000"
        android:layout_centerHorizontal="true"
        android:id="@+id/hasiljudul"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="#000000"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/hasiljudul"
        android:layout_marginTop="10dp"
        android:id="@+id/hasilnama"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="#000000"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/hasilnama"
        android:layout_marginTop="10dp"
        android:id="@+id/hasilemail"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="#000000"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/hasilemail"
        android:layout_marginTop="10dp"
        android:id="@+id/hasilpasssword"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="#000000"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/hasilpasssword"
        android:layout_marginTop="10dp"
        android:id="@+id/hasiljeniskelamin"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="#000000"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/hasiljeniskelamin"
        android:layout_marginTop="10dp"
        android:id="@+id/hasilusia"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="#000000"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/hasilusia"
        android:layout_marginTop="10dp"
        android:id="@+id/hasilprodi"
        />
</RelativeLayout>